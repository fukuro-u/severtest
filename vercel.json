 {
    "version": 2,
    "builds": [
      {
        "src": "**/*",
        "use": "@vercel/static",
        "config": {
          "output": "build"
        }
      }
    ],
    "framework": "vercel",
    "routes": [
      {
        "src": "/(.*)",
        "dest": "build/server.js" 
      }
    ],
    "environment": {
      "NODE_VERSION": "18.x"
    }
  }
